/**
 * Lists are represented by means of the constructors Nil and Cons.
 */
module collection/list/cons
signature
  sorts List(a)
  constructors
  
    /**
     * Empty list.
     */
    Nil  : List(a)
    
    /**
     * Element of a list.
     */
    Cons : a * List(a) -> List(a)
    
    /**
     * Concatenation of two lists.
     */
    Conc : List(a) * List(a) -> List(a)
