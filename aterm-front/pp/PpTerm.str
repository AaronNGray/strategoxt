module PpTerm
imports options ugly-print pp-aterm-options

strategies

  io-PpTerm =
    io-wrap(max-term-size-option,
      ugly-print
    ; concat-content
    )
    
signature
  constructors
    Conc : List(a) * List(a) -> List(a)

strategies 

  concat-content = 
    bottomup( try( \ Conc(l1, l2) -> <conc> (l1, l2) \ ))
    
  
  