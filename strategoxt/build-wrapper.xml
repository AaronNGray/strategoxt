<?xml version="1.0"?>
<project name="org.strategoxt.strategoxt-wrapper" default="all">

	<property name="baseline" value="" />
	<property name="sdf2bundle" value="" />
	<property name="nativepath" value="" />
	
	<property name="strjbaseline-base" value="" />
	<property name="strjbaseline-deps" value="" />
	
	<property name="build-target" value="" />

	<target name="all">
		<java classname="org.apache.tools.ant.launch.Launcher" fork="true" failonerror="true">
			<jvmarg value="-Xss8m" />
			<jvmarg value="-Xmx1024m" />
			<classpath>
				<pathelement location="${ant.home}/lib/ant-launcher.jar" />
			</classpath>
			<arg value="-buildfile" />
			<arg file="build.xml" />
			<arg value="-Dbaseline=${baseline}" />
			<arg value="-Dsdf2bundle=${baseline}" />
			<arg value="-Dnativepath=${nativepath}" />
			<arg value="-lib" />
			<arg value="${strjbaseline-base}" />
			<arg value="-lib" />
			<arg value="${strjbaseline-deps}" />
		</java>
	</target>

</project>