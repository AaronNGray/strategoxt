module URI.generated

completions
  completion template Segment : "ID:Term" =
    <ID:ID> ":" <Term:Term>  
  completion template Segment : "ID:Term#ID" =
    <ID:ID> ":" <Term:Term> "#" <ID:ID>  
  completion template Segment : "anon ID" =
    "anon " <ID:ID>  
  completion template Segment : "subsq ID" =
    "subsq " <ID:ID>  