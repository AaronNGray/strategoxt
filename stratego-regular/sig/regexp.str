module regexp

signature
  sorts RegExp(a)
  constructors
    Empty  : RegExp(a)
    Sym    : a -> RegExp(a)
    Star   : RegExp(a) -> RegExp(a)
    Plus   : RegExp(a) -> RegExp(a)
    Seq    : RegExp(a) * RegExp(a) -> RegExp(a)
    Choice : RegExp(a) * RegExp(a) -> RegExp(a)
    Opt    : RegExp(a) -> RegExp(a)
