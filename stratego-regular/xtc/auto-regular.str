module auto-regular
imports xtc-lib

strategies

  main-auto-regular =
      install-regular-makefile
    ; install-regular-m4
    ; <exit> 0

  install-regular-makefile =
      <xtc-find-file> "Makefile.regular"
    ; copy-to(!"Makefile.regular")

  install-regular-m4 =
      <xtc-find-file> "stratego-regular.m4"
    ; copy-to(!"stratego-regular.m4")
