module C-TypeMach
imports C

strategies

  is-C-TranslationUnit =
      ?TranslationUnit(_)

  is-C-TranslationUnitItem =
      is-String
    + ?FunDef(_, _, _)
    + ?Declaration2(_, _)

  is-C-IdentifierDeclarator =
      ?IdDecl(_, _, _)

  is-C-Identifier = 
      ?Id(_)

  is-String = is-string
