module singleast

imports
	
	analysis/new-analysis
	nabl/collect
	
rules
	
	analysis-singleast = analysis-singleast(nabl-collect)
	analysis-singleast(collector):
		ast -> (analyzed-ast, (error*, warning*, note*))
		with
			language     := "Language"
		; path         := "path"
		; project-path := "."
    ; partition := <analysis-partition(|project-path)> path
	  ; Result([FileResult(_, _, _, _, analyzed-ast, error*, warning*, note*)], _, _, _) := 
	    	<analysis-files(id, id, id|language, project-path)> [File(partition, ast)]
