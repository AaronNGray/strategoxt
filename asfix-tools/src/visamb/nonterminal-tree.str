module nonterminal-tree
imports lib AsFix verbalize-sdf
strategies

  nonterminal-tree =
    io-wrap(
      topdown(try(ApplToSort + LitToString))
    )

  ApplToSort :
    appl(prod(as, a, _), ts) -> c#(ts)
      where <![["\""],<Sym2Strs>,["\""]]; concat; concat-strings> a => c

  LitToString :
    lit(s) -> s
