module Index

imports

	Term

templates

	Section.Index =
<index <Term>

	<Entry*; separator="\n">
>

	Entry.Def    = <def <Term>>
	Entry.Use    = <use <Term>>
	Entry.Prop   = <prop <Term> -- <Term> \>\> <Term>>
	Entry.Alias  = <alias <Term> -\> <Term>>
	Entry.IAlias = <alias <Term> \<- <Term>>
	
	Term        = <<Entry>>
