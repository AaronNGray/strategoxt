module Instruction.pp.generated

strategies
  prettyprint-MsgType :
    Note() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("note")]
                )
              ]

  prettyprint-MsgType :
    Warn() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("warning")]
                )
              ]

  prettyprint-MsgType :
    Err() -> [ H(
                 [SOpt(HS(), "0")]
               , [S("error")]
               )
             ]

  prettyprint-MsgTrigger :
    Fail() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("fails")]
                )
              ]

  prettyprint-MsgTrigger :
    Succ() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("succeeds")]
                )
              ]

  prettyprint-MsgTrigger :
    Mult() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("has multiple results")]
                )
              ]

  prettyprint-Instruction :
    Msg(a, b, c, d, e) -> [ H(
                              [SOpt(HS(), "0")]
                            , [ S("show ")
                              , a506
                              , S(" ")
                              , b199
                              , S(" on ")
                              , c155
                              , S(" when ")
                              , d101
                              , S(" ")
                              , e73
                              ]
                            )
                          ]
    where a506 := <pp-one-Z(prettyprint-MsgType)> a
    where b199 := <pp-one-Z(prettyprint-Term)> b
    where c155 := <pp-one-Z(prettyprint-Term)> c
    where d101 := <pp-one-Z(prettyprint-Term)> d
    where e73 := <pp-one-Z(prettyprint-MsgTrigger)> e

  prettyprint-Instruction :
    Choice(a) -> [ H(
                     [SOpt(HS(), "0")]
                   , [S("choose "), a507]
                   )
                 ]
    where a507 := <pp-H-list(prettyprint-Term|" <+ ")> a

  prettyprint-Instruction :
    Sequence(a) -> [ H(
                       [SOpt(HS(), "0")]
                     , [S("sequence "), a508]
                     )
                   ]
    where a508 := <pp-H-list(prettyprint-Term|" ; ")> a

  prettyprint-Instruction :
    Combine(a) -> [ H(
                      [SOpt(HS(), "0")]
                    , [S("concat "), a509]
                    )
                  ]
    where a509 := <pp-H-list(prettyprint-Term|" + ")> a

  prettyprint-Instruction :
    Id(a) -> [ H(
                 [SOpt(HS(), "0")]
               , [S("produce "), a510]
               )
             ]
    where a510 := <pp-one-Z(prettyprint-Term)> a

  prettyprint-Instruction :
    PropLookup(a, b) -> [ H(
                            [SOpt(HS(), "0")]
                          , [ S("lookup ")
                            , a511
                            , S(" prop on ")
                            , b200
                            ]
                          )
                        ]
    where a511 := <pp-one-Z(prettyprint-Term)> a
    where b200 := <pp-one-Z(prettyprint-Term)> b

  prettyprint-Instruction :
    PropsLookup(a, b) -> [ H(
                             [SOpt(HS(), "0")]
                           , [ S("lookup ")
                             , a512
                             , S(" props on ")
                             , b201
                             ]
                           )
                         ]
    where a512 := <pp-one-Z(prettyprint-Term)> a
    where b201 := <pp-one-Z(prettyprint-Term)> b

  prettyprint-Instruction :
    PropCheck(a, b, c, d) -> [ H(
                                 [SOpt(HS(), "0")]
                               , [ S("check ")
                                 , a513
                                 , S(" prop ")
                                 , b202
                                 , S(" against ")
                                 , c156
                                 , S(" wrt ")
                                 , d102
                                 ]
                               )
                             ]
    where a513 := <pp-one-Z(prettyprint-Term)> a
    where b202 := <pp-one-Z(prettyprint-Term)> b
    where c156 := <pp-one-Z(prettyprint-Term)> c
    where d102 := <pp-one-Z(prettyprint-Term)> d

  prettyprint-Instruction :
    Rewrite(a, b) -> [ H(
                         [SOpt(HS(), "0")]
                       , [ S("rewrite ")
                         , a514
                         , S(" wrt ")
                         , b203
                         ]
                       )
                     ]
    where a514 := <pp-one-Z(prettyprint-Term)> a
    where b203 := <pp-one-Z(prettyprint-Term)> b

  prettyprint-Instruction :
    Match(a, b, c) -> [ H(
                          [SOpt(HS(), "0")]
                        , [ S("match ")
                          , a515
                          , S(" using relation ")
                          , b204
                          , S(" wrt bindings ")
                          , c157
                          ]
                        )
                      ]
    where a515 := <pp-one-Z(prettyprint-Term)> a
    where b204 := <pp-one-Z(prettyprint-Term)> b
    where c157 := <pp-one-Z(prettyprint-Term)> c

  prettyprint-Instruction :
    ResolveDefs(a, b, c, d) -> [ H(
                                   [SOpt(HS(), "0")]
                                 , [ S("resolve ")
                                   , a516
                                   , S(" ")
                                   , b205
                                   , S(" definition in ")
                                   , c158
                                   , S(" wrt props ")
                                   , d103
                                   ]
                                 )
                               ]
    where a516 := <pp-one-Z(prettyprint-Term)> a
    where b205 := <pp-one-Z(prettyprint-Term)> b
    where c158 := <pp-one-Z(prettyprint-Term)> c
    where d103 := <pp-one-Z(prettyprint-Term)> d

  prettyprint-example =
    prettyprint-MsgType

  prettyprint-example =
    prettyprint-MsgTrigger

  prettyprint-example =
    prettyprint-Instruction