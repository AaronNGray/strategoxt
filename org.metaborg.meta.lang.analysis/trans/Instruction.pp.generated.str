                                 )
                               ]
    where a522 := <pp-one-Z(prettyprint-Term)> a
    where b182 := <pp-one-Z(prettyprint-Term)> b
    where c75 := <pp-one-Z(prettyprint-Term)> c
    where d46 := <pp-one-Z(prettyprint-Term)> d

  prettyprint-Instruction :
    CheckDefs(a, b) -> [ H(
                           [SOpt(HS(), "0")]
                         , [ S("check ")
                           , a523
