  implode-tests-concrete-yield =
      where(
        <ReadFromFile> "Concrete.tbl"; open-parse-table => tbl
      )
    ; set-default-sglr-config
     
    ; let tester = 
            parse-string-pt(|tbl)
            ; implode-asfix(false)
            ; yield-asfix-quotes
    in
    
      apply-and-check(!"Simple yielded quotation with asfix content."
