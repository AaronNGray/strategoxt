definition

module Main
imports
  Regular
  
module Regular
exports 
  context-free syntax
    A*		-> A {cons("a1")}
    A+		-> A {cons("a2")}
    {A ","}*	-> A {cons("a3")}
    (A B C)	-> A {cons("a4")}
    A?		-> A {cons("a5")}
    A | B	-> A {cons("a6")}
    A # B	-> A {cons("a7")}
    [A-Z]	-> A {cons("a8")}
    label:A	-> A {cons("a9")}
