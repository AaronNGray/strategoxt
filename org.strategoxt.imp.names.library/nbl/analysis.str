module nbl/analysis

imports
	
	nbl/collect
	nbl/tasks
	
strategies
	
	nbl-analyse:
		ast -> (ast', elems', errs)
		where
			(ast', coll)   := <nbl-collect>; 
			(tasks, elems) := <split-tasks> coll;
			tasks'         := <merge-tasks> tasks;
			(errs, elems') := <perform-tasks> (tasks', elems)
			
	