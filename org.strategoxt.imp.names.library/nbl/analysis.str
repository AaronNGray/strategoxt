module nbl/analysis

imports
	
	nbl/collect
	nbl/tasks
	
strategies
	
	nbl-analyse:
		ast -> (ast', elems, errs)
		where
			elem-set := <new-iset>;
			task-set := <new-iset>;
			(ast', tasks)  := <nbl-collect(|elem-set, task-set)>; 
			tasks'         := <merge-tasks> tasks;
			elems					 := <iset-elements> elem-set;
			errs  				 := <perform-tasks(|elems)> tasks'
			
	