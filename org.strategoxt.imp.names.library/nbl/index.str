module index

imports
	
	nbl/collect
	nbl/utils
	
rules
	
	new-def(|elems, uri):
		x -> definition
		where
			definition := <force-origins(!Def(uri))> x;
			<iset-add(|definition)> elems
			
	new-use(|elems, uri):
		x -> use
		where
			use := <force-origins(!Use(uri))> x;
			<iset-add(|use)> elems
		
	new-prop(|elems, uri, kind, val):
		x -> prop
		where
			prop := <force-origins(!Prop(uri, kind, val))> x;
			<iset-add(|prop)> elems
