module nbl/incremental

imports
	
	index/query
	nbl/uri
      
rules
  
  nbl-begin-collect-reads = 
      rules(NBL-ReadSet := <new-iset>)
      
  nbl-end-collect-reads:
    _ -> elements
    with
    	if set := <NBL-ReadSet> then
    	  elements := <iset-elements> set
    	else
    		elements := []
    	end;
    	rules(NBL-ReadSet :- _)
