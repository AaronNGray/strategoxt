module lr_ok_8
signature
  sorts A B
  constructors
  : B
  : B * B -> B
  : B -> A

attributes
  @ (B)():
    root.lr_test_syn := root.lr_test_inh

  @ (B * B -> B)(f, s):
    s.lr_test_inh := f.lr_test_syn    // add
    f.lr_test_inh := root.lr_test_inh // add
    root.lr_test_syn := s.lr_test_syn // add

  @ (B -> A)(b):
    b.lr_test_inh := 42
    root.ok := b.lr_test_syn
