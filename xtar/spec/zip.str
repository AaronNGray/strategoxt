module zip
imports list string

strategies

  get-zip-entries	=
    ?name ; prim("SSL_get_zip_entries", name)
		
  file-from-zip =
    ?(name, filename) ; prim("SSL_file_from_zip", name, filename, filename)

  file-from-zip =
    ?(name, filename, asname) ; prim("SSL_file_from_zip", name, filename, asname)
		
  file-to-zip =
    ?(name, filename) ; prim("SSL_file_to_zip", name, filename, filename)

  file-to-zip =
    ?(name, filename, asname) ; prim("SSL_file_to_zip", name, filename, asname)
				
  aterm-from-zip =
    ?(name, termname) ; prim("SSL_aterm_from_zip", name, termname)
		
  aterm-to-zip =
    ?(name, termname, term) ; prim("SSL_aterm_to_zip", name, termname, term)

  open-zip =
    ?(name, mode) ; prim("SSL_open_zip", name, mode)
		
  close-zip =
    ?name ; prim("SSL_close_zip", name)