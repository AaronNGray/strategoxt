module term-to-adot-test
imports sunit term-to-adot
signature
  constructors
    F : a * a -> a
    B : a
    C : a

strategies

  term-to-adot-test =
    test-suite(!"term-to-adot-test",
	term-to-tree-test
    )

  term-to-tree-test =
    apply-test(!"term-to-tree-test"
	, term-to-tree
	, !F(B,C)
	, ![("a_0",F("b_0","c_0")),("b_0",B),("c_0",C)]
	)
