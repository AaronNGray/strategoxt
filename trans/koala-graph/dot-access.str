\begin{code}
module dot-access
imports Dot


overlays
/* Overlays for "old" Dot abstract syntax (part of graph-tools package) */
/*
   dot-edge(left, right, attrs) =
   edge-stmt(left, arrow, [right], Attr-List(attrs) )
   
   dot-node(name, attrs ) =
   node-stmt(name, Attr-List(attrs))
   
   dot-subgraph( name, stms ) =
   sub-graph(name, Some(stms))
   
   dot-digraph(name, stms) =
   dot-graph( "", alt(2, []), name, stms)
   
   dot-graph-attrs(attrs) =
   graph(attrs)
   
   dot-node-attrs(attrs) = 
   node(attrs)
   
   dot-alias(name, value) =
   alias(name, value)
   
   dot-attr-id(name, value) =
   attr-id(name, value)
   
   dot-attr-num-con(name, value) =
   attr-num-con(name, value)
*/
   /* Overlays for "new" Dot abstract syntax (part of dot-tools package) */

   dot-edge(left, right, attrs) =
   Edge(Id(left), [Arrow(Id(right))], Attrs(attrs) )
   
   dot-node(name, attrs ) =
   Node(Id(name), Attrs(attrs))
   
   dot-subgraph( name, stms ) =
   SubGraph(name, stms)
   
   dot-digraph(name, stms) =
   DotDigraph(name, stms)
   
   dot-graph-attrs(attrs) =
   GraphAttrs(attrs)
   
   dot-node-attrs(attrs) = 
   NodeAttrs(attrs)
   
   dot-alias(name, value) =
   Alias(name, value)
   
   dot-attr-id(name, value) =
   AttrId(name, value)
   
   dot-attr-num-con(name, value) =
   AttrNumCon(name, value)


\end{code}
