/**
 * Some degree of XTC and native calling compatibility.
 *
 * @author Lennart Kats
 */
module xtc-compat

imports
  libstratego-lib

strategies

  override xtc-find =
    warn-msg(|[<whoami <+ !"application">, " uses xtc to find ", <id>])
  
  override call =
    ?(program, args);
    prim("SSL_EXT_call", program, args)
