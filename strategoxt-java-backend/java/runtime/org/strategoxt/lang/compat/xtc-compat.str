/**
 * Some degree of XTC and native calling compatibility.
 *
 * @author Lennart Kats
 */
module xtc-compat

imports
  libstratego-lib

strategies

  override xtc-find =
    warn(|[<whoami <+ !"application">, "uses xtc"])
  
  override call =
    ?(program, args);
    prim("SSL_EXT_call", program, args)
